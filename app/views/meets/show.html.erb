<div class=show_meet>
<hr>
<table>
  <tr>
    <td class=meet_left_header><b><%= @meet.name %></b></td>
    <td class=meet_right_header><b><%= @meet.date %></b></td>
  </tr>
</table>
<hr>
<% @results.each do |course, results| %>
  <% next if @results[course].size == 0 %>
  <% f = @results[course].first %>
  <h2><%= course %></h2>
  <table align=center>
    <tr><td colspan=5>
      Length = <%= f.length %>, Climb = <%= f.climb %>, Controls = <%= f.controls %>
    </td></tr>
    <tr>
      <th>Runner</th><th>School</th><th>Place</th><th>Classifier</th><th>Time</th><th>Gender</th>
    </tr>
    <% @results[course].each do |r| %>
      <tr>
        <% runner = Runner.find(r.runner_id) %>
        <td><%= runner.name %>(<%= runner.id %>)</td>
        <td><%= runner.club_description %></td>
        <td><%= r.place %></td>
        <td><%= r.classifier %></td>
        <td><%= r.time.round(3) %></td>
        <td><%= r.gender %></td>
      </tr>
    <% end %>
  </table>
<% end %>
</div>
