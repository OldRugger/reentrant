<div class=calc_runs >
  <hr>
  <table border=0>
    <tr>
      <th class=calc >HS Rankings</th>
      <th class=news >News</th>
      <th class=links >Links</th>
   </tr>
  </table>
  <hr>
  <table border=0>
   <tr>
     <td class=calc>
      <table class=calc_run >
        <% @calc_runs.each do |calc_run| %>
          <tr>
            <td>
              <%= link_to calc_run.date, calc_run %>
            </td>
          <tr>
        <% end %>
      </table
     </td>
     <td>
      <table class=news >
        <% @news.each do |news_item| %>
          <tr>
            <td class=date >
              <%= news_item.date %>
            </td>
            <td>
              <%= sanitize(news_item.text) %>
            </td>
          <tr>
        <% end %>
      </table
     </td>
     <td class=links>
      <table >
        <tr><td><b><%= link_to 'High School Splits', split_course_index_path %></b></td></tr>
        <tr><td><b><%= link_to 'High School Power Rankings', power_ranking_show_path(id: CalcRun.where(publish: true).last.id)  %></b> </td></tr>
        <tr><td><b><%= link_to 'All Runners', runners_path %></b> <br><br></td></tr>
        <tr><td>  ---- External Links ----  </td></tr>
        <% @links.each do |link| %>
          <tr><td><%= link_to link.label, link.url %></td></tr>
        <% end %>
      </table>
     </td>
   </tr>
  </table>
</div>