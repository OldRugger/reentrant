<!DOCTYPE html>
<html>
<head>
  <title><%= APP_CONFIG[:title] %></title>
  <%= favicon_link_tag 'favicon.ico' %>
  <%= stylesheet_link_tag 'bootstrap.min' %>
  <%= javascript_include_tag "charts_loader.js" %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <table class=app_header>
    <tr >
      <td >
        <div class= "container">
          <%= link_to calc_runs_path do %>
            <% image_tag "reentrant.png", :style => "display: block;" %>
          <% end %>
          <%= link_to calc_runs_path do %>
            <div class="title">
              Reentrant
            </div>
            <div class="subtitle">
              Reporting on Georgia High School Orienteering
            </div>
          <% end %>

        </div>
        
      </td>
    </tr>
  </table>

<%= yield %>
<%= javascript_include_tag 'popper.min.js' %>
<%= javascript_include_tag 'bootstrap.min.js'%>

</body>
</html>
